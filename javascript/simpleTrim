function simpleTrim(src){
	function trim(a){
		var arr=a.split("");
		for(var i=0;i<arr.length;i++){
			if(arr[i]==" "){
				arr.splice(i,1);
				i--;
			}
			else
			break;
		}
	arr.reverse();
	return arr.join("");
	}
var b=trim(trim(src));
return b;
}

